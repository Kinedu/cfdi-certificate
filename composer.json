{
    "name": "kinedu/cfdi-certificate",
    "description": "Librería para decodificar los Certificados de Sello Digital (CSD) del Servicio de Administración Tributaria (SAT)",
    "homepage": "https://github.com/Kinedu/cfdi-certificate",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Kinedu",
        "cfdi",
        "sat"
    ],
    "authors": [
        {
            "name": "Orlando Charles",
            "email": "orlando@kinedu.com",
            "homepage": "https://orlandocharles.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Kinedu\\CFDI\\Certificate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kinedu\\CFDI\\Certificate\\Tests\\": "tests"
        }
    },
    "prefer-stable": true
}
